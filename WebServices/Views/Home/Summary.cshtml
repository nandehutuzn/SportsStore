@model IEnumerable<WebServices.Models.Reservation>

<div class="panel-heading">Rreservation</div>
<div class="panel-body">
    <table class="table table-striped table-condensed">
        <thead>
            <tr><th>ID</th><th>Name</th><th>Location</th><th></th></tr>
        </thead>
        <tbody>
            @foreach (var item in Model) {
            <tr>
                <td>@item.ReservationId</td>
                <td>@item.ClientName</td>
                <td>@item.Location</td>
                <td>@Html.ActionLink("Remove", "Remove", new { id=item.ReservationId}, new { @class="btn btn-xs btn-primary"})</td>
            </tr>
            }
        </tbody>
    </table>
</div>
